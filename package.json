    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persist": "^1.0.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "url": "^0.11.0",
    "vue": "3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.1",
    "vue-types": "^5.0.3",
    "web-storage-cache": "^1.1.1"
